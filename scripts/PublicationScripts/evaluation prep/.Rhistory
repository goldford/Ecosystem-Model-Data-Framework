# === Task 2: ICES/Perry-Style Seasonal Anomaly Calculation (CSOG only) ===
# 2025-12-18 THIS REVISION CONSIDERED after thinking a bit about using the _df_ or
# _df_summarised_ in first lines
# cross-checked original outputs - not a major difference
set.seed(1)
df_csog_tows <- df_summarised %>%
filter(Region == "Central Strait of Georgia")
# seasonal mean across tows (only where group exists in a tow)
seasonal_means <- df_csog_tows %>%
group_by(Year, Season, ModelGroup) %>%
summarise(BCarbon_g_m2 = mean(BCarbon_g_m2, na.rm = TRUE), .groups="drop")
# Perry-style zero handling ONLY for true zeros that exist in the computed means
# (this will be rare if absence is mostly missing rows)
seasonal_means2 <- seasonal_means %>%
group_by(ModelGroup) %>%
mutate(
min_pos = suppressWarnings(min(BCarbon_g_m2[BCarbon_g_m2 > 0], na.rm=TRUE)),
min_pos = ifelse(is.infinite(min_pos), NA_real_, min_pos),
BCarbon_g_m2_adj = ifelse(BCarbon_g_m2 == 0 & !is.na(min_pos),
runif(n(), 0, 0.5 * min_pos),
BCarbon_g_m2),
Log10_B = log10(BCarbon_g_m2_adj)
) %>%
ungroup() %>%
select(-min_pos)
clim <- seasonal_means2 %>%
group_by(ModelGroup, Season) %>%
summarise(Climatology = mean(Log10_B, na.rm=TRUE), .groups="drop")
seasonal_anoms <- seasonal_means2 %>%
left_join(clim, by=c("ModelGroup","Season")) %>%
mutate(SeasonalAnomaly = Log10_B - Climatology)
seasonal_wide <- seasonal_anoms %>%
select(Year, Season, ModelGroup, SeasonalAnomaly) %>%
pivot_wider(names_from = ModelGroup, values_from = SeasonalAnomaly)
write.csv(
seasonal_wide,
paste0(path, "Zooplankton_Carbon_SeasonalAnomalies_CSoG_Wide.csv"),
row.names = FALSE
)
# Visual diagnostic
ggplot(seasonal_anoms, aes(
x = Year + match(Season, c("Winter", "Spring", "Summer", "Fall")) / 5,
y = SeasonalAnomaly,
color = ModelGroup
)) +
geom_line() + geom_point() +
facet_wrap(~ModelGroup, scales = "free_y") +
theme_minimal() +
labs(
title = "Seasonal Carbon Biomass Anomalies - CSoG (tow-mean, Perry-style zeros)",
x = "Year",
y = "Seasonal Anomaly (log10 biomass - seasonal climatology)"
)
